{
  "manifest_version": 2,

  "name": "GitHub Default Monospace Font",
  "version": "1.0.0",
  "description": "Use your browser's configured monospace font on Github",

  "content_scripts": [
    {
      "css": ["styles.css"],
      "matches": [
        "https://github.com/*",
        "https://*.github.com/*",
        "https://*.github.dev/*"
      ]
    }
  ]
}
